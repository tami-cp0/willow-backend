model Seller {
  userId      String   @id @map("user_id")
  user        User     @relation(fields: [userId], references: [id], onDelete: Cascade)

  username    String   @db.VarChar(20)
  bio         String   @db.VarChar(255)

  sales    Sale[]
  products Product[]
  conversations        Conversation[]

  @@map("sellers")
}